<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>DarkPan - Bootstrap 5 Admin Template</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
    <link href="img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600&family=Roboto:wght@500;700&display=swap" rel="stylesheet"> 
    
    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax//admin/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- /admin/Libraries Stylesheet -->
    <link href="/admin/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="/admin/lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />

    <!-- Customized Bootstrap Stylesheet -->
    <link href="/admin/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="/admin/css/style.css" rel="stylesheet">

    <!-- FontAwsome  -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>

<body>
    <div class="container-fluid position-relative d-flex p-0">
        <!-- Spinner Start -->
        <div id="spinner" class="show bg-dark position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
            <div class="spinner-border text-primary" style="width: 3rem; height: 3rem;" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        </div>
        <!-- Spinner End -->


        <!-- Sidebar Start -->
        <div class="sidebar pe-4 pb-3">
            <nav class="navbar bg-secondary navbar-dark">
                <a href="index.html" class="navbar-brand mx-4 mb-3">
                    <h3 class="text-primary"><i class="fa fa-user-edit me-2"></i>DarkPan</h3>
                </a>
                <div class="d-flex align-items-center ms-4 mb-4">
                    <div class="position-relative">
                        <img class="rounded-circle" src="img/user.jpg" alt="" style="width: 40px; height: 40px;">
                        <div class="bg-success rounded-circle border border-2 border-white position-absolute end-0 bottom-0 p-1"></div>
                    </div>
                    <div class="ms-3">
                        <h6 class="mb-0">KRISHNAPRASAD</h6>
                        <span>Admin</span>
                    </div>
                </div>
                <div class="navbar-nav w-100">
                    <a href="/admin/dashboard" class="nav-item nav-link active"><i class="fa fa-tachometer-alt me-2"></i>Dashboard</a>
                  
                    <a href="/admin/alluser" class="nav-item nav-link"><i class="fa fa-th me-2"></i>All User</a>
                    <div class="nav-item dropdown">
                        <a href="/admin/allproduct" class="nav-link dropdown-toggle "><i class="far fa-file-alt me-2"></i>All Product</a>
                        <div class="dropdown-menu bg-transparent border-0">
                            <a href="/admin/addproduct" class="dropdown-item">Add Product</a>
                        </div>
                    </div>
                    <div class="nav-item dropdown">
                        <a href="/admin/category" class="nav-link dropdown-toggle" ><i class="far fa-file-alt me-2"></i>Category</a>
                        <div class="dropdown-menu bg-transparent border-0">
                            <a href="/admin/addcategory" class="dropdown-item">Add Category</a>
                        </div>
                    </div>

                    <a href="/admin/orders" class="nav-item nav-link"><i class="fa fa-th me-2"></i>Orders</a>

                    <div class="nav-item dropdown">
                        <a href="/admin/coupon" class="nav-link dropdown-toggle "><i
                                class="far fa-file-alt me-2"></i>All Coupons</a>
                        <div class="dropdown-menu bg-transparent border-0">
                            <a href="/admin/addcoupon" class="dropdown-item">Add Coupon</a>
                        </div>
                    </div>

                    <div class="nav-item dropdown">
                        <a href="/admin/banner" class="nav-link dropdown-toggle"><i
                                class="far fa-file-alt me-2"></i>All Banners</a>
                        <div class="dropdown-menu bg-transparent border-0">
                            <a href="/admin/addbanner" class="dropdown-item">Add Banner</a>
                        </div>
                    </div>
                  
                </div>
                <a href="/admin/logout" class="nav-item nav-link"><i class="fa fa-chart-bar me-2"></i>Logout</a>
            </nav>
        </div>
        <!-- Sidebar End -->


        <!-- Content Start -->
        <div class="content">
            <!-- Navbar Start -->
            <nav class="navbar navbar-expand bg-secondary navbar-dark sticky-top px-4 py-0">
                <a href="index.html" class="navbar-brand d-flex d-lg-none me-4">
                    <h2 class="text-primary mb-0"><i class="fa fa-user-edit"></i></h2>
                </a>
                <a href="#" class="sidebar-toggler flex-shrink-0">
                    <i class="fa fa-bars"></i>
                </a>
                <form class="d-none d-md-flex ms-4">
                    <input class="form-control bg-dark border-0" type="search" placeholder="Search">
                </form>
              
            </nav>
            <!-- Navbar End -->


             <!-- Sale & Revenue Start -->
             <div class="container-fluid pt-4 px-4">
                <div class="row g-4">
                    <div class="col-sm-6 col-xl-3">
                        <div class="bg-secondary rounded d-flex align-items-center justify-content-between p-4">
                            <i class="fa fa-chart-line fa-3x text-primary"></i>
                            <div class="ms-3">
                                <p class="mb-2">Razorpay and Wallet Revenue</p>
                                <h6 class="mb-0">₹<%= revenue.length > 0 ? revenue[0].totalrevenue : 0 %></h6>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-xl-3">
                        <div class="bg-secondary rounded d-flex align-items-center justify-content-between p-4">
                            <i class="fa fa-chart-line fa-3x text-primary"></i>
                            <div class="ms-3">
                                <p class="mb-2">Cash On Delivery Revenue</p>
                                <h6 class="mb-0">₹ <%= codRevenue.length > 0 ? codRevenue[0].total : 0 %></h6>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-xl-3">
                        <div class="bg-secondary rounded d-flex align-items-center justify-content-between p-4">
                            <i class="fa fa-chart-bar fa-3x text-primary"></i>
                            <div class="ms-3">
                                <p class="mb-2">Total Revenue</p>
                                <h6 class="mb-0">₹<%= revenuelen %></h6>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-xl-3">
                        <div class="bg-secondary rounded d-flex align-items-center justify-content-between p-4">
                            <i class="fa fa-chart-bar fa-3x text-primary"></i>
                            <div class="ms-3">
                                <p class="mb-2">All Orders</p>
                                <h6 class="mb-0"><%= salesLen %></h6>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-xl-3">
                        <div class="bg-secondary rounded d-flex align-items-center justify-content-between p-4">
                            <i class="fa fa-chart-area fa-3x text-primary"></i>
                            <div class="ms-3">
                                <p class="mb-2">Pending Orders</p>
                                <h6 class="mb-0"><%= pendlen %></h6>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-xl-3">
                        <div class="bg-secondary rounded d-flex align-items-center justify-content-between p-4">
                            <i class="fa fa-chart-pie fa-3x text-primary"></i>
                            <div class="ms-3">
                                <p class="mb-2">All Products</p>
                                <h6 class="mb-0"><%= productlen %></h6>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-xl-3">
                        <div class="bg-secondary rounded d-flex align-items-center justify-content-between p-4">
                            <i class="fa fa-chart-pie fa-3x text-primary"></i>
                            <div class="ms-3">
                                <p class="mb-2">Total User</p>
                                <h6 class="mb-0"><%= totalUser %></h6>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-xl-3">
                        <div class="bg-secondary rounded d-flex align-items-center justify-content-between p-4">
                            <i class="fa fa-chart-pie fa-3x text-primary"></i>
                            <div class="ms-3">
                                <p class="mb-2">All category</p>
                                <h6 class="mb-0"><%= categorylen %></h6>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


            <!-- Sales Chart Start -->
            <div class="container-fluid pt-4 px-4">
                <div class="row g-4">
                    <div class="col-sm-12 col-xl-6">
                        <div class="bg-secondary text-center rounded p-4">
                            <div class="d-flex align-items-center justify-content-between mb-4">
                                <h6 class="mb-0">Monthly Sales</h6>
                                <a href="">Show All</a>
                            </div>
                            <canvas id="worldwide-sales"></canvas>
                        </div>
                    </div>
                    <div class="col-sm-12 col-xl-6">
                        <div class="bg-secondary text-center rounded p-4">
                            <div class="d-flex align-items-center justify-content-between mb-4">

                                <h6 class="mb-0">Yearly Sales</h6>
                                <a href="">Show All</a>
                            </div>
                            <canvas id="salse-revenue"></canvas>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Sales Chart End -->


            <div class="container-fluid pt-4 px-4">
                <div class="bg-secondary text-center rounded p-4">
                    <div class="d-flex align-items-center justify-content-between mb-4">
                        <h6 class="mb-0">New members</h6>
                        <a href="">Show All</a>
                    </div>
                    <div class="table-responsive">
                        <table class="table text-start align-middle table-bordered table-hover mb-0">
                            <thead>
                                <tr class="text-white">
                                   
                                    <th scope="col">Name</th>
                                    <th scope="col">Mobile</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Wallet Balance</th>
                                </tr>
                            </thead>
                            <tbody>

                                <% user.forEach((user) => { %>
                                    <tr>
                                      <td><%= user.name %></td>
                                      <td><%= user.email %></td>
                                      <td><%= user.mobile %></td>
                                      <td>₹<%= user.wallet %></td>
                                    </tr>
                                  <% }); %>
                                  
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <br>

            <a href="/admin/salesreport" class="btn btn-primary ms-4">Sales Report</a>



            <!-- Widgets Start -->
         


            <!-- Footer Start -->
            <div class="container-fluid pt-4 px-4">
                <div class="bg-secondary rounded-top p-4">
                    <div class="row">
                        <div class="col-12 col-sm-6 text-center text-sm-start">
                            &copy; <a href="#">Your Site Name</a>, All Right Reserved. 
                        </div>
                        <div class="col-12 col-sm-6 text-center text-sm-end">
                            <!--/*** This template is free as long as you keep the footer author’s credit link/attribution link/backlink. If you'd like to use the template without the footer author’s credit link/attribution link/backlink, you can purchase the Credit Removal License from "https://htmlcodex.com/credit-removal". Thank you for your support. ***/-->
                            Designed By <a href="https://htmlcodex.com">HTML Codex</a>
                            <br>Distributed By: <a href="https://themewagon.com" target="_blank">ThemeWagon</a>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Footer End -->
        </div>
        <!-- Content End -->


        <!-- sales -->

        <input id="monthlyOrdersChart1" type="hidden" value="<%=updatedMonthlyValues[0].total %>">
        <input id="monthlyOrdersChart2" type="hidden" value="<%=updatedMonthlyValues[1].total %>">
        <input id="monthlyOrdersChart3" type="hidden" value="<%=updatedMonthlyValues[2].total %>">
        <input id="monthlyOrdersChart4" type="hidden" value="<%=updatedMonthlyValues[3].total %>">
        <input id="monthlyOrdersChart5" type="hidden" value="<%=updatedMonthlyValues[4].total %>">
        <input id="monthlyOrdersChart6" type="hidden" value="<%=updatedMonthlyValues[5].total %>">
        <input id="monthlyOrdersChart7" type="hidden" value="<%=updatedMonthlyValues[6].total %>">
        <input id="monthlyOrdersChart8" type="hidden" value="<%=updatedMonthlyValues[7].total %>">
        <input id="monthlyOrdersChart9" type="hidden" value="<%=updatedMonthlyValues[8].total %>">
        <input id="monthlyOrdersChart10" type="hidden" value="<%=updatedMonthlyValues[9].total %>">
        <input id="monthlyOrdersChart11" type="hidden" value="<%=updatedMonthlyValues[10].total %>">
        <input id="monthlyOrdersChart12" type="hidden" value="<%=updatedMonthlyValues[11].total %>">



         <!-- Orders -->
        <input id="monthlyOrdersChart25" type="hidden" value="<%=updatedMonthlyValues[0].count %>">
        <input id="monthlyOrdersChart26" type="hidden" value="<%=updatedMonthlyValues[1].count %>">
        <input id="monthlyOrdersChart27" type="hidden" value="<%=updatedMonthlyValues[2].count %>">
        <input id="monthlyOrdersChart28" type="hidden" value="<%=updatedMonthlyValues[3].count %>">
        <input id="monthlyOrdersChart29" type="hidden" value="<%=updatedMonthlyValues[4].count %>">
        <input id="monthlyOrdersChart30" type="hidden" value="<%=updatedMonthlyValues[5].count %>">
        <input id="monthlyOrdersChart31" type="hidden" value="<%=updatedMonthlyValues[6].count %>">
        <input id="monthlyOrdersChart32" type="hidden" value="<%=updatedMonthlyValues[7].count %>">
        <input id="monthlyOrdersChart33" type="hidden" value="<%=updatedMonthlyValues[8].count %>">
        <input id="monthlyOrdersChart34" type="hidden" value="<%=updatedMonthlyValues[9].count %>">
        <input id="monthlyOrdersChart35" type="hidden" value="<%=updatedMonthlyValues[10].count %>">
        <input id="monthlyOrdersChart36" type="hidden" value="<%=updatedMonthlyValues[11].count %>">


        <!-- Users -->
        <input id="monthlyOrdersChart13" type="hidden" value="<%=updatedMonthlyUserCount[0].count %>">
        <input id="monthlyOrdersChart14" type="hidden" value="<%=updatedMonthlyUserCount[1].count %>">
        <input id="monthlyOrdersChart15" type="hidden" value="<%=updatedMonthlyUserCount[2].count %>">
        <input id="monthlyOrdersChart16" type="hidden" value="<%=updatedMonthlyUserCount[3].count %>">
        <input id="monthlyOrdersChart17" type="hidden" value="<%=updatedMonthlyUserCount[4].count %>">
        <input id="monthlyOrdersChart18" type="hidden" value="<%=updatedMonthlyUserCount[5].count %>">
        <input id="monthlyOrdersChart19" type="hidden" value="<%=updatedMonthlyUserCount[6].count %>">
        <input id="monthlyOrdersChart20" type="hidden" value="<%=updatedMonthlyUserCount[7].count %>">
        <input id="monthlyOrdersChart21" type="hidden" value="<%=updatedMonthlyUserCount[8].count %>">
        <input id="monthlyOrdersChart22" type="hidden" value="<%=updatedMonthlyUserCount[9].count %>">
        <input id="monthlyOrdersChart23" type="hidden" value="<%=updatedMonthlyUserCount[10].count %>">
        <input id="monthlyOrdersChart24" type="hidden" value="<%=updatedMonthlyUserCount[11].count %>">



        <!-- chart 2 -->

        <input type="hidden" id="YearlySalesData" value="<%= updatedYearlyValues[0].total %>">
        <input type="hidden" id="YearlySalesData1" value="<%= updatedYearlyValues[1].total %>">
        <input type="hidden" id="YearlySalesData2" value="<%= updatedYearlyValues[2] .total%>">
        <input type="hidden" id="YearlySalesData3" value="<%= updatedYearlyValues[3] .total%>">

        <input type="hidden" id="YearlySalesData4" value="<%= updatedYearlyValues[0].count %>">
        <input type="hidden" id="YearlySalesData5" value="<%= updatedYearlyValues[1].count %>">
        <input type="hidden" id="YearlySalesData6" value="<%= updatedYearlyValues[2].count %>">
        <input type="hidden" id="YearlySalesData7" value="<%= updatedYearlyValues[3].count %>">

       


        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

    <!-- JavaScript /admin/Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="/admin/lib/chart/chart.min.js"></script>
    <script src="/admin/lib/easing/easing.min.js"></script>
    <script src="/admin/lib/waypoints/waypoints.min.js"></script>
    <script src="/admin/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="/admin/lib/tempusdominus/js/moment.min.js"></script>
    <script src="/admin/lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="/admin/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

    <!-- Template Javascript -->
    <script src="/admin/js/main.js"></script>
</body>

</html>