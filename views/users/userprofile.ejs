                                                              <div class="d-flex flex-row mb-4 pb-2 mt-4 ">
                                                                <div class="flex-fill">
                                                                  <h5 class="bold"> <a href="/eachproduct/?id=<%= product.productId._id %>"><%= product.name %></a></h5>
                                                                  <p class="text-muted"> Qt: <%= product.quentity %> item</p>
                                                                  <h4 class="mb-3">  ₹ <%= product.total %> <span class="small text-muted"> <br> via (<%=order.paymentMode%>) </span></h4>
                                                                </div>
                                                                <div>
                                                                  <img class="align-self-center img-fluid" src="/uploads/<%=product.image%>" width="150" height="100">
                                                                </div>
                                                              </div>
                                                              <% if(product.orderStatus=="placed"){%>
                                                                    <p class="text-primary"><%=product.orderStatus%></p>
                                                              <div class="card-footer p-4">
                                                                <div class="d-flex justify-content-between">
                                                                  <h5 class="fw-normal mb-0">
                                                                    <a href="#" class="mb-4" onclick="removefromorder('<%=order._id%>' , '<%=product._id%>','<%= product.orderStatus %>')">Cancel</a>
                                                                  </h5>
                                                                  <div class="border-start h-100"></div>
                                                                </div>
                                                              </div>
                                                              <%}%>
                                                                <%if(product.orderStatus=="shipped"){%>
                                                                <h6 class="text-primary " style="margin-left: 20px; margin-bottom: 30px;" ><%=product.orderStatus%></h6>
                                                                <div class="card-footer p-4">
                                                                    <div class="d-flex justify-content-between">
                                                                      <h5 class="fw-normal mb-0">
                                                                        <a href="#" class="mb-4" onclick="removefromorder('<%=order._id%>' , '<%=product._id%>','<%= product.orderStatus %>')">Cancel</a>
                                                                      </h5>
                                                                      <div class="border-start h-100"></div>
                                                                    </div>
                                                                  </div>
                                                                <%}%>

